{
  "env": {
    "commonjs": {
      "presets": [
        ["env", { "loose": true }],
        "stage-2"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "transform-runtime"
      ]
    },
    "es": {
      "presets": [
        ["env", { "loose": true, "modules": false }],
        "stage-2"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "transform-runtime"
      ]
    },
    "dist": {
      "presets": [
        ["env", { "loose": true, "modules": false }],
        "stage-2"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "external-helpers",
        "transform-runtime"
      ]
    }
  }
}