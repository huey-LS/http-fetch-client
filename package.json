{
  "name": "http-fetch-client",
  "version": "2.0.12",
  "description": "a http client for browser",
  "keywords": [
    "fetch",
    "http",
    "request",
    "browser"
  ],
  "files": [
    "lib",
    "es",
    "index.js",
    "index.d.ts"
  ],
  "typings": "index.d.ts",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "test": "cross-env BABEL_ENV=commonjs nyc mocha",
    "clean": "rm -fr dist lib es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:dist": "cross-env BABEL_ENV=dist rollup -c rollup.config.js",
    "build": "npm run clean && npm run build:commonjs && npm run build:es && npm run build:dist",
    "prepare": "npm run build",
    "coverage": "codecov"
  },
  "prepublish": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/ignous/http-fetch-client.git"
  },
  "author": "ignous",
  "email": "lsxyz7@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "cross-env": "^5.0.5",
    "mocha": "^3.5.3",
    "nyc": "^14.1.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.4",
    "rollup-plugin-node-resolve": "^3.0.0",
    "sinon": "^3.2.1"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "querystringify": "^2.1.1",
    "url-parse": "^1.2.0"
  }
}
