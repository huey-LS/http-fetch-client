{
  "name": "http-fetch-client",
  "version": "0.1.0",
  "description": "a http client",
  "files": [
    "lib",
    "es",
    "src",
    "index.js"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "webpack_build": "webpack --config ./webpack.config.js",
    "webpack_dev": "npm run webpack_build",
    "webpack": "export NODE_ENV=production || set NODE_ENV=production && npm run webpack_build",
    "clean": "rm -fr dist lib es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run clean && npm run build:commonjs && npm run build:es",
    "prepare": "npm run build"
  },
  "prepublish": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/ignous/http-fetch-client.git"
  },
  "author": "ignous",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.0.5",
    "webpack": "^3.5.5"
  },
  "dependencies": {}
}
