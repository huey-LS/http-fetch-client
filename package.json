{
  "name": "http-fetch-client",
  "version": "0.3.3",
  "description": "a http client for browser",
  "keywords": [
    "fetch",
    "http",
    "request",
    "browser"
  ],
  "files": [
    "lib",
    "es",
    "src",
    "index.js"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "test": "npm run build && mocha",
    "clean": "rm -fr dist lib es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:dist": "cross-env BABEL_ENV=dist rollup -c rollup.config.js",
    "build": "npm run clean && npm run build:commonjs && npm run build:es && npm run build:dist",
    "prepare": "npm run build",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov -t c8a760ab-1e49-4990-b455-3c0582845f68"
  },
  "prepublish": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/ignous/http-fetch-client.git"
  },
  "author": "ignous",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.0.5",
    "mocha": "^3.5.3",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.4",
    "rollup-plugin-node-resolve": "^3.0.0",
    "sinon": "^3.2.1"
  },
  "dependencies": {
    "is-generator-function": "^1.0.6"
  }
}
